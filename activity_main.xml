<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnRedmond"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Redmond"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        android:width="150dp"
        android:height="150dp"/>

    <Button
        android:id="@+id/btnSammamish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sammamish"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="30dp"
        android:layout_marginTop="50dp"
        android:width="150dp"
        android:height="150dp"/>


    <Button
        android:id="@+id/btnBothell"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bothell"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnRedmond"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        android:width="150dp"
        android:height="150dp"/>

    <Button
        android:id="@+id/btnBellevue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bellevue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnSammamish"
        android:layout_marginRight="30dp"
        android:layout_marginTop="50dp"
        android:width="150dp"
        android:height="150dp"/>

    <Button
        android:id="@+id/btnSeattle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Seattle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnBothell"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        android:width="150dp"
        android:height="150dp"/>

    <Button
        android:id="@+id/btnIssaquah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Issaquah"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnBellevue"
        android:layout_marginRight="30dp"
        android:layout_marginTop="50dp"
        android:width="150dp"
        android:height="150dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>